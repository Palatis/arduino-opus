{
	"name": "esp8266-opus",
	"keywords": "audio, codec, opus, lossy",
	"description": "The opus audio codec from XiPH.org, precompiled for esp8266-arduino.",
	"authors":
	{
		"name": "Victor Tseng",
		"email": "palatis@gmail.com",
		"url": "https://github.com/Palatis/esp8266-arduino-opus"
	},
	"version": "0.1",
	"downloadUrl": "https://github.com/Palatis/esp8266-arduino-opus/releases/download/v0.1/esp8266-arduino-opus-v0.1.tar.xz",
	"frameworks": "arduino",
	"platforms": [ "espressif8266" ],

	"export": {
		"include": [
			"include/*"
		]
	},

	"build": {
		"flags": [
			"-I include",
			"-I src",
			"-L lib",
			"-lopus",
			"-lopusfile"
		]
	}
}
